@using Bookish.Models;
@model AddMemberModel;

<form asp-controller="Members" asp-action="AddMember" method="post">
    <label asp-for="FirstName">First name: </label>
    <input type="text" name="firstname" asp-for="FirstName"></input>

    <label asp-for="LastName">Last name: </label>
    <input type="text" name="lastname" asp-for="LastName"></input>

     <label asp-for="Address">Address: </label>
    <input type="text" name="address" asp-for="Address"></input>

    <button type="submit">Submit</button>
</form>